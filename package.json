{
  "name": "vue-map-ui",
  "version": "0.0.7",
  "description": "Map component library based on leaflet for Vue 3",
  "author": "Nikolay Naumenkov <nikolas.it.web@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/nikolaynau/vue-map-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaynau/vue-map-ui.git"
  },
  "bugs": {
    "url": "https://github.com/nikolaynau/vue-map-ui/issues"
  },
  "keywords": [
    "map ui",
    "map",
    "component library",
    "ui framework",
    "ui",
    "vue3",
    "leaflet"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./*": "./*"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{js,ts,vue,css}": "prettier --write"
  },
  "scripts": {
    "watch": "vite build --watch",
    "build": "run-s type-check build-only build-dts",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "build-dts": "vue-tsc -p tsconfig.dts.json",
    "build-styles": "postcss src/assets/css/normalize.css --base src/assets/css --dir dist -u \"postcss-import\" tailwindcss autoprefixer cssnano --no-map",
    "build-all": "run-s build build-styles global-dts",
    "global-dts": "esno scripts/gen-global.ts",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.{js,ts,vue,css}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:cov": "vitest run --coverage",
    "postversion": "npm run push",
    "push": "git push origin master --tags",
    "husky": "husky install"
  },
  "peerDependencies": {
    "leaflet": ">=1.9.0",
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^9.12.0",
    "@vueuse/shared": "^9.12.0",
    "object.omit": "^3.0.0",
    "object.pick": "^1.3.0",
    "vue-use-leaflet": "^0.0.5"
  },
  "devDependencies": {
    "@babel/types": "^7.20.7",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-angular": "^17.4.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.0",
    "@types/leaflet": "^1.9.0",
    "@types/node": "^18.11.19",
    "@types/object.omit": "^3.0.0",
    "@types/object.pick": "^1.3.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.10",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "eslint": "^8.33.0",
    "eslint-plugin-vue": "^9.9.0",
    "esno": "^0.16.3",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "leaflet": "^1.9.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.4",
    "vue": "^3.2.47",
    "vue-tsc": "^1.0.24"
  }
}
